li=[]
n=int(input("enter the number of elements in the list : "))
print("Enter the elements of the list : ")
for i in range(n):
    a=int(input())
    if(a>0):
        li.append(a)
for i in range(1,max(li)):
    if i in li:
        pass
    else:
        print("First Missing positive Integer is",i)
        break
